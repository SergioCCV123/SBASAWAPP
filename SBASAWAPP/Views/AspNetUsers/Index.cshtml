@model IEnumerable<SBASAWAPP.AspNetUser>

@{ Layout = "~/Views/Shared/_AdminLayout.cshtml"; }

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@

<section id="main">
    <div class="inner">

        <section id="one" class="wrapper style1">
            <header class="special">
                <h2>Perfiles de Usuario</h2>
            </header>
            <div class="content">
                <table class="table" id="MiTabla" style="width:100%">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayName("Correo")
                            </th>
                            <th>
                                @Html.DisplayName("Tel√©fono")
                            </th>
                            <th>
                                @Html.DisplayName("Nombre Usuario")
                            </th>
                            <th>
                                @Html.DisplayName("Bloqueo")
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Email)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PhoneNumber)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.LockoutEnabled)
                            </td>
                            <td>
                                @Html.ActionLink("Detalles", "Details", new { id = item.Id })
                            </td>
                            <td>
                                @Html.ActionLink("Editar", "Edit", new { id = item.Id })
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </section>
    </div>
</section>
