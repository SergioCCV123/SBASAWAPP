@model SBASAWAPP.AspNetUser

@{ Layout = "~/Views/Shared/_AdminLayout.cshtml"; }

@{
    ViewBag.Title = "Detalle";
}

<h2>Detalle</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Correo electronico")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        
        <dt>
            @Html.DisplayName("Número telefónico")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>

       
        <dt>
            @Html.DisplayName("Autenticación en dos pasos")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TwoFactorEnabled)
        </dd>

        <dt>
            @Html.DisplayName("Bloqueo Habilitado")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LockoutEnabled)
        </dd>

        <dt>
            @Html.DisplayName("Nombre de usuario")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Activo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Activo)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Volver a la lista", "Index")
</p>